{
  "name": "electron-vite-template",
  "productName": "Electron Vite Template",
  "private": true,
  "version": "2.1.0",
  "description": "Electron Vite Template app.",
  "license": "MIT",
  "homepage": "./",
  "author": {
    "name": "Wesley",
    "email": "wesleyara.contato@gmail.com"
  },
  "main": "dist-electron/electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "format": "prettier --write src",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "axios": "^1.2.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.6.1",
    "react-toastify": "^9.1.1",
    "utils-react": "^1.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "electron": "^21.1.0",
    "electron-builder": "^23.3.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-electron-plugin": "^0.5.2",
    "vite-plugin-electron-renderer": "^0.11.3"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "prettier --write src",
      "eslint --ext js,ts,jsx,tsx src --fix"
    ]
  }
}
